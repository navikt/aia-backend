{
  "name": "aia-backend",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev src/index.ts | pino-pretty",
    "test": "jest --watch",
    "test:ci": "npm run lint && jest --passWithNoTests",
    "lint": "eslint --ext .ts src/**",
    "prettier": "prettier --write 'src/**/*.ts'",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
    "prepare": "husky install",
    "prisma:migrate": "prisma migrate dev"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@elastic/ecs-pino-format": "1.3.0",
    "@prisma/client": "4.5.0",
    "axios": "1.1.3",
    "body-parser": "1.20.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "helmet": "6.0.0",
    "jose": "4.10.3",
    "jsonwebtoken": "8.5.1",
    "node-jose": "2.1.1",
    "openid-client": "5.2.1",
    "pino": "8.7.0",
    "pino-http": "8.2.1",
    "swagger-jsdoc": "6.2.5",
    "swagger-ui-express": "4.5.0",
    "ulid": "2.3.0",
    "unleash-client": "3.16.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.0",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.11.4",
    "@types/node-jose": "1.1.10",
    "@types/supertest": "2.0.12",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "8.26.0",
    "husky": "8.0.1",
    "jest": "29.2.1",
    "jest-mock-extended": "3.0.1",
    "lint-staged": "13.0.3",
    "pino-pretty": "9.1.1",
    "prettier": "2.7.1",
    "supertest": "6.3.0",
    "ts-jest": "29.0.3",
    "ts-node-dev": "2.0.0",
    "typescript": "4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aia-backend.git"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 4
  },
  "lint-staged": {
    "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
  },
  "engines": {
    "node": ">=16.18.0"
  },
  "license": "MIT"
}
