{
  "name": "aia-backend",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev src/index.ts | pino-pretty",
    "test": "vitest dev",
    "test:ci": "npm run lint && vitest run",
    "lint": "eslint",
    "prettier": "prettier --write 'src/**/*.ts'",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
    "prepare": "husky",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@alheimsins/uker-fra-dato": "1.0.4",
    "@elastic/ecs-pino-format": "1.5.0",
    "@navikt/oasis": "4.1.1",
    "@prisma/client": "7.1.0",
    "axios": "1.13.2",
    "body-parser": "2.2.1",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "express": "5.2.1",
    "helmet": "8.1.0",
    "jose": "6.1.3",
    "pino": "10.1.0",
    "pino-http": "11.0.0",
    "prom-client": "15.1.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "unleash-client": "6.9.0",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/body-parser": "1.19.6",
    "@types/cookie-parser": "1.4.10",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/node": "25.0.2",
    "@types/supertest": "6.0.3",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.8",
    "@types/uuid": "11.0.0",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "eslint": "9.39.2",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "pino-pretty": "13.1.3",
    "prettier": "3.7.4",
    "prisma": "7.1.0",
    "supertest": "7.1.4",
    "ts-jest": "29.4.6",
    "ts-node-dev": "2.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
    "vitest": "4.0.15",
    "vitest-mock-extended": "3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aia-backend.git"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 4
  },
  "lint-staged": {
    "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
  },
  "engines": {
    "node": ">=22.20.0"
  },
  "license": "MIT"
}
