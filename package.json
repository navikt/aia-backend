{
  "name": "aia-backend",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev src/index.ts | pino-pretty",
    "test": "jest --watch",
    "test:ci": "npm run lint && jest --passWithNoTests",
    "lint": "eslint",
    "prettier": "prettier --write 'src/**/*.ts'",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
    "prepare": "husky",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "graphql:generate": "graphql-codegen"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@alheimsins/uker-fra-dato": "1.0.4",
    "@apollo/server": "4.11.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@graphql-tools/graphql-file-loader": "8.0.1",
    "@graphql-tools/load": "8.0.2",
    "@graphql-tools/schema": "10.0.5",
    "@navikt/oasis": "3.3.0",
    "@prisma/client": "5.18.0",
    "axios": "1.7.4",
    "body-parser": "1.20.3",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "graphql": "16.9.0",
    "helmet": "7.1.0",
    "jose": "5.6.3",
    "jsonwebtoken": "9.0.2",
    "node-jose": "2.2.0",
    "openid-client": "5.6.5",
    "pino": "9.3.2",
    "pino-http": "10.2.0",
    "prom-client": "15.1.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "ulid": "2.3.0",
    "unleash-client": "6.1.1",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.9.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript-resolvers": "4.2.1",
    "@types/body-parser": "1.19.5",
    "@types/cookie-parser": "1.4.7",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "22.2.0",
    "@types/node-jose": "1.1.13",
    "@types/supertest": "6.0.2",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.6",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.1.0",
    "@typescript-eslint/parser": "8.1.0",
    "eslint": "9.9.0",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.7",
    "lint-staged": "15.2.9",
    "pino-pretty": "11.2.2",
    "prettier": "3.3.3",
    "prisma": "5.18.0",
    "supertest": "7.0.0",
    "ts-jest": "29.2.4",
    "ts-node-dev": "2.0.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aia-backend.git"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 4
  },
  "lint-staged": {
    "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
  },
  "engines": {
    "node": ">=20.16.0"
  },
  "license": "MIT"
}
