{
  "name": "aia-backend",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev src/index.ts | pino-pretty",
    "test": "jest --watch",
    "test:ci": "npm run lint && jest --passWithNoTests",
    "lint": "eslint --ext .ts src/**",
    "prettier": "prettier --write 'src/**/*.ts'",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
    "prepare": "husky install",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@alheimsins/uker-fra-dato": "1.0.4",
    "@elastic/ecs-pino-format": "1.3.0",
    "@prisma/client": "4.16.2",
    "axios": "1.4.0",
    "body-parser": "1.20.2",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "jose": "4.14.4",
    "jsonwebtoken": "9.0.1",
    "kafkajs": "2.2.4",
    "node-jose": "2.2.0",
    "openid-client": "5.4.3",
    "pino": "8.15.0",
    "pino-http": "8.4.0",
    "prom-client": "14.2.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0",
    "ulid": "2.3.0",
    "unleash-client": "4.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "18.16.3",
    "@types/node-jose": "1.1.10",
    "@types/supertest": "2.0.12",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "eslint": "8.47.0",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-mock-extended": "3.0.5",
    "lint-staged": "14.0.1",
    "pino-pretty": "10.2.0",
    "prettier": "3.0.2",
    "prisma": "4.16.2",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aia-backend.git"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 4
  },
  "lint-staged": {
    "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "license": "MIT"
}
