{
  "name": "aia-backend",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev src/index.ts | pino-pretty",
    "test": "jest --watch",
    "test:ci": "npm run lint && jest --passWithNoTests",
    "lint": "eslint",
    "prettier": "prettier --write 'src/**/*.ts'",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
    "prepare": "husky",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@alheimsins/uker-fra-dato": "1.0.4",
    "@elastic/ecs-pino-format": "1.5.0",
    "@navikt/oasis": "3.8.0",
    "@prisma/client": "6.16.2",
    "axios": "1.12.2",
    "body-parser": "2.2.0",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dotenv": "17.2.2",
    "express": "5.1.0",
    "helmet": "8.1.0",
    "jose": "6.1.0",
    "pino": "9.11.0",
    "pino-http": "10.5.0",
    "prom-client": "15.1.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "unleash-client": "6.7.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@types/body-parser": "1.19.6",
    "@types/cookie-parser": "1.4.9",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.5.2",
    "@types/supertest": "6.0.3",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.8",
    "@types/uuid": "11.0.0",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "eslint": "9.36.0",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "jest-mock-extended": "4.0.0",
    "lint-staged": "16.2.0",
    "pino-pretty": "13.1.1",
    "prettier": "3.6.2",
    "prisma": "6.16.2",
    "supertest": "7.1.4",
    "ts-jest": "29.4.4",
    "ts-node-dev": "2.0.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aia-backend.git"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 4
  },
  "lint-staged": {
    "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
  },
  "engines": {
    "node": ">=22.20.0"
  },
  "license": "MIT"
}
