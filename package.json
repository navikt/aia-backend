{
  "name": "aia-backend",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev src/index.ts | pino-pretty",
    "test": "jest --watch",
    "test:ci": "npm run lint && jest --passWithNoTests",
    "lint": "eslint",
    "prettier": "prettier --write 'src/**/*.ts'",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
    "prepare": "husky",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@alheimsins/uker-fra-dato": "1.0.4",
    "@elastic/ecs-pino-format": "1.5.0",
    "@navikt/oasis": "3.7.0",
    "@prisma/client": "6.6.0",
    "axios": "1.8.4",
    "body-parser": "1.20.3",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "dotenv": "16.5.0",
    "express": "4.21.2",
    "helmet": "8.1.0",
    "jose": "5.10.0",
    "pino": "9.6.0",
    "pino-http": "10.4.0",
    "prom-client": "15.1.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "ulid": "2.3.0",
    "unleash-client": "6.6.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "@types/body-parser": "1.19.5",
    "@types/cookie-parser": "1.4.8",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.14",
    "@types/node": "22.14.1",
    "@types/supertest": "6.0.3",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.8",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.31.0",
    "@typescript-eslint/parser": "8.31.0",
    "eslint": "9.25.1",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.7",
    "lint-staged": "15.5.1",
    "pino-pretty": "13.0.0",
    "prettier": "3.5.3",
    "prisma": "6.6.0",
    "supertest": "7.1.0",
    "ts-jest": "29.3.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/aia-backend.git"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 4
  },
  "lint-staged": {
    "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
  },
  "engines": {
    "node": ">=20.17.0"
  },
  "license": "MIT"
}
